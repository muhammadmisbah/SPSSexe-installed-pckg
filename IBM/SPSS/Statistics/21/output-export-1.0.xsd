<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:oex="http://www.ibm.com/software/analytics/spss/xml/output-export"
           targetNamespace="http://www.ibm.com/software/analytics/spss/xml/output-export"
           elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
  <!-- the root element -->
  <xs:element name="outputExport">
    <xs:annotation>
      <xs:documentation xml:lang="en">
        Top level element.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="oex:outputExportRequest" />
          <xs:element ref="oex:outputExportResponse" />
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="outputExportRequest">
    <xs:annotation>
      <xs:documentation xml:lang="en">
                Request to output element, describing how Statistics should export the contents of a
                Statistics output viewer document.
            </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="oex:contents" />
        <xs:choice minOccurs="0" maxOccurs="1">
          <xs:element ref="oex:doc" />
          <xs:element ref="oex:html" />
          <xs:element ref="oex:pdf" />
          <xs:element ref="oex:ppt" />
          <xs:element ref="oex:text" />
          <xs:element ref="oex:xls" />
          <xs:element ref="oex:xlsx" />
          <xs:element ref="oex:xlsm" />
        </xs:choice>
        <xs:choice minOccurs="0" maxOccurs="1">
          <xs:element ref="oex:bmp" />
          <xs:element ref="oex:emf" />
          <xs:element ref="oex:eps" />
          <xs:element ref="oex:jpg" />
          <xs:element ref="oex:png" />
          <xs:element ref="oex:tif" />
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="name" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
              The NAME keyword specifies the Viewer document whose contents will be exported.
              This keyword is optional. If NAME is omitted, the contents of the designated Viewer
              document will be exported.

              The NAME keyword is useful when more than one Viewer document is open and you want to
              ensure that the OUTPUT EXPORT command operates on the correct document.
            </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the CONTENTS subcommand -->
  <xs:element name="contents">
    <xs:annotation>
      <xs:documentation>
          The CONTENTS element is used to specify which Viewer contents to export.
          This element is optional.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="export" use="required">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The EXPORT keyword specifies whether to export all Viewer items or only those that are visible.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="all">
              <xs:annotation>
                <xs:documentation>
                          Export all Viewer items.  All Viewer items will be exported regardless of
                          whether they are visible or hidden. This is the default.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="visible">
              <xs:annotation>
                <xs:documentation>
                          Export visible items only.  Only visible Viewer items will be exported;
                          hidden items will not be exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="layers" use="required">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The LAYERS keyword specifies how to export pivot tables that have layers.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="printSetting">
              <xs:annotation>
                <xs:documentation>
                          Export layers as specified for printing in Table Properties.  This is the
                          default choice. The Printing tab in the Table Properties dialog includes a
                          check box option to print all layers of a pivot table; by default, only the
                          visible layer is printed. If you specify LAYERS=PRINTSETTING, this check box
                          determines how the pivot table is exported. If the box is not checked (default),
                          only the visible layer of the table will be exported; if the box is checked, all
                          layers will be exported. Each layer will display as a separate table.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="all">
              <xs:annotation>
                <xs:documentation>
                          Export all layers.  All layers of each pivot table will be exported.
                          Each layer will display as a separate table.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="visible">
              <xs:annotation>
                <xs:documentation>
                          Export visible layer only.  The visible layer of each pivot table will be
                          exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="modelviews" use="required">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The MODELVIEWS keyword specifies which model views to export. All views – even those that
            appear as tables in the Model Viewer – are exported as images. Each view is exported as a
            separate image.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="printSetting">
              <xs:annotation>
                <xs:documentation>
                          Export views as specified for printing in the Model Viewer.  This is the default
                          choice. The Model Properties dialog in the Model Viewer includes a check box
                          option to print all views of a model; by default, only the view that is
                          currently visible is printed. If you specify MODELVIEWS=PRINTSETTING, the
                          check box determines how the model is exported. If the box is not checked
                          (default), only the view that is currently visible will be exported; if the
                          box is checked, all views will be exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="all">
              <xs:annotation>
                <xs:documentation>
                          Export all views.  All views of all models will be exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="visible">
              <xs:annotation>
                <xs:documentation>
                          Export visible view only.  Only the visible view of each model will be exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the DOC subcommand -->
  <xs:element name="doc">
    <xs:annotation>
      <xs:documentation>
        The DOC subcommand is used to specify options for exporting to Word/RTF documents (*.doc).
        Pivot tables are exported as Word tables with all formatting attributes intact - for example,
        cell borders, font styles, and background colors. Text output is exported as formatted RTF.
        On Windows operating systems, graphics are included in the document in EMF (enhanced metafile)
        format. On other operating systems, graphics are included in PNG format.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="notesCaptions" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The notesCaptions keyword specifies whether to export footnotes and captions for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pageBreaks" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The pageBreaks keyword specifies whether to preserve page breaks and keep together settings for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="wideTables" default="wrap">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The wideTables attribute specifies how to export tables that are too wide to fit within the
            margins specified for the Word/RTF document.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="wrap">
              <xs:annotation>
                <xs:documentation>
                                    Wrap wide tables.  Statistics will break the table after the last column that fits
                                    completely within the margins. The remaining columns will be placed below the
                                    first part of the table. This is the default choice.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="shrink">
              <xs:annotation>
                <xs:documentation>
                                    Shrink wide tables to fit within the margins.  Statistics will reduce the font size
                                    and remove white space in order to make a wide table fit within the margins.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="asis">
              <xs:annotation>
                <xs:documentation>
                          Do not shrink or wrap wide tables.  The table may extend outside the margin
                          and page boundaries when exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="pageSizeUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The pageSizeUnits attribute specifies the units used for page dimensions to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pageSizeWidth" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The pageSizeWidth attribute specifies the page width dimension to use for the exported
            document. The orientation of the page is implied by the dimensions. Specifying a width
            that is less than the height is equivalent to choosing portrait orientation for the page.
            Specifying a width that is greater than the height is equivalent to choosing landscape
            orientation.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pageSizeHeight" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The pageSizeHeight attribute specifies the page width dimension to use for the exported
            document. The orientation of the page is implied by the dimensions. Specifying a width
            that is less than the height is equivalent to choosing portrait orientation for the page.
            Specifying a width that is greater than the height is equivalent to choosing landscape
            orientation.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="topMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The topMarginUnits attribute specifies the units used for top margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="topMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the bottom margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bottomMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The bottomMarginUnits attribute specifies the units used for bottom margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bottomMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the bottom margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leftMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The leftMarginUnits attribute specifies the units used for left margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leftMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the left margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rightMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The rightMarginUnits attribute specifies the units used for right margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rightMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the right margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the HTML subcommand -->
  <xs:element name="html">
    <xs:annotation>
      <xs:documentation>
        The HTML element is used to specify options for exporting to HTML documents (*.htm).
        Pivot tables are exported as HTML tables. Text output is exported as preformatted HTML.
        Graphics are embedded by reference; you should export them in a suitable format for inclusion
        in HTML documents (for example, PNG and JPG).
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="styling" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The styling keyword specifies whether to export pivot tables with styling and column widths or not.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="notesCaptions" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The notesCaptions keyword specifies whether to export footnotes and captions for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="imageFormat" type="oex:imageFormat" default="JPG">
        <xs:annotation>
          <xs:documentation>
            The imageFormat attribute specifies the image file format to use for exported images.
            A link to each image file will be placed in the HTML document.
            The filename specified for the documentfile attribute is used as the rootname for the image
            files. A numeral is appended to the rootname to distinguish multiple image files from one
            another (e.g., MyOutput0.png, MyOutput1.png, etc).
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the PDF subcommand -->
  <xs:element name="pdf">
    <xs:annotation>
      <xs:documentation>
        The PDF element is used to specify options for exporting to PDF documents (*.pdf). Your current
        Print settings are honored. That is, output is exported as it appears in Print Preview, with all
        formatting attributes intact.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="embedBookmarks" default="true" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            The embedBookmarks attribute specifies whether to include bookmarks in the PDF document
            that correspond to the Viewer outline entries. Like the Viewer outline pane, bookmarks
            can make it much easier to navigate documents with a large number of output objects.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="embedFonts" default="true" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
            The embedFonts attribute specifies whether to embed fonts in the exported document.
            Embedding fonts ensures that the PDF document will look the same on all computers.
            Otherwise, if some fonts used in the document are not available on the computer being
            used to view (or print) the PDF document, font substitution may yield suboptimal results.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the PPT subcommand -->
  <xs:element name="ppt">
    <xs:annotation>
      <xs:documentation>
        The PPT element is used to specify options for exporting to PowerPoint documents. Pivot tables
        are exported as Word tables and are embedded on separate slides in the PowerPoint file, with one
        slide for each pivot table. All formatting attributes of the pivot table are retained - for
        example, cell borders, font styles, and background colors. Text output is exported as formatted
        RTF. Graphics are embedded in the document in TIF format. An error is issued if the PPT
        subcommand is used on a non-Windows system or on a system that does not have PowerPoint installed.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="notesCaptions" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The notesCaptions keyword specifies whether to export footnotes and captions for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="slideTitles" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The slideTitles attribute specifies whether to include a title on each slide that is
            created by the export. Each slide contains a single item that is exported from the Viewer.
            The title is formed from the outline entry for the item in the outline pane of the Viewer.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="wideTables" default="wrap">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The wideTables attribute specifies how to export tables that are too wide to fit within the
            margins specified for the Word/RTF document.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="wrap">
              <xs:annotation>
                <xs:documentation>
                                    Wrap wide tables.  Statistics will break the table after the last column that fits
                                    completely within the margins. The remaining columns will be placed below the
                                    first part of the table. This is the default choice.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="shrink">
              <xs:annotation>
                <xs:documentation>
                                    Shrink wide tables to fit within the margins.  Statistics will reduce the font size
                                    and remove white space in order to make a wide table fit within the margins.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="asis">
              <xs:annotation>
                <xs:documentation>
                          Do not shrink or wrap wide tables.  The table may extend outside the margin
                          and page boundaries when exported.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="slideSizeUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The slideSizeUnits attribute specifies the units used for slide dimensions to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="slideSizeWidth" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The slideSizeWidth attribute specifies the slide width dimension to use for the exported
            document. The orientation of the slide is implied by the dimensions. Specifying a width
            that is less than the height is equivalent to choosing portrait orientation for the slide.
            Specifying a width that is greater than the height is equivalent to choosing landscape
            orientation.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="slideSizeHeight" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The slideSizeHeight attribute specifies the slide width dimension to use for the exported
            document. The orientation of the slide is implied by the dimensions. Specifying a width
            that is less than the height is equivalent to choosing portrait orientation for the slide.
            Specifying a width that is greater than the height is equivalent to choosing landscape
            orientation.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="topMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The topMarginUnits attribute specifies the units used for top margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="topMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the bottom margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bottomMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The bottomMarginUnits attribute specifies the units used for bottom margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="bottomMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the bottom margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leftMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The leftMarginUnits attribute specifies the units used for left margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="leftMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the left margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rightMarginUnits" type="oex:measurementUnits">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The rightMarginUnits attribute specifies the units used for right margin to use for the
            exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="rightMargin" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Specifies the right margin for the exported document.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the TEXT subcommand -->
  <xs:element name="text">
    <xs:annotation>
      <xs:documentation>
        The text element is used to specify options for exporting to text documents (*.txt). Text output
        formats include local encoding, UTF-8, and UTF-16. Pivot tables can be exported in tab-separated
        or space-separated format. All text output is exported in space-separated format. For graphics,
        a line is inserted in the text file indicating the filename of the exported image.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="encoding" default="localEncoding">
        <xs:annotation>
          <xs:documentation>
            The encoding attribute specifies the type of text file to create.  For users running Statistics in
            Unicode mode, the localEncoding and utf8 options both result in UTF8 text
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="localEncoding">
              <xs:annotation>
                <xs:documentation>
                                    Export in the character set used for the Statistics locale. This is the default choice.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="utf8">
              <xs:annotation>
                <xs:documentation>
                          Export as 8-bit Unicode text. This is the most widely-used Unicode encoding.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="utf16">
              <xs:annotation>
                <xs:documentation>
                          Export as 16-bit Unicode text.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="notesCaptions" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The notesCaptions keyword specifies whether to export footnotes and captions for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="autoFit" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Make each column as wide as the widest label or value in that column. Column contents will
            not be wrapped.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="columnWidth" type="xs:integer">
        <xs:annotation>
          <xs:documentation>
            The columnWidth attribute specifies the width of columns in pivot tables exported to text.
            It is valid only if separator=space is also specified; and autoFit is false.  Otherwise, it
            is ignored.  Set the maximum column width for all columns to w characters. Provide an integer
            value for w. If a cell’s contents are wider than w characters, the contents will be wrapped
            onto the next line in that column.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="separator" default="space">
        <xs:annotation>
          <xs:documentation>
            The separator attribute specifies whether to use spaces or tabs to separate columns.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="space">
              <xs:annotation>
                <xs:documentation>
                          Use spaces to separate columns. This is the default choice.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="tab">
              <xs:annotation>
                <xs:documentation>
                          Use tabs to separate columns.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="rowBorder" type="xs:string" default="-">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The rowBorder attribute specifies the character to use to create row borders in pivot tables.
            It is valid only if separator=space is also specified; otherwise, it is ignored.
            The default character is the hyphen. To suppress display of row borders, specify a blank space
            as the character to use.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="columnBorder" type="xs:string" default="|">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The columnBorder attribute specifies the character to use to create column borders in pivot
            tables. It is valid only if separator=space is also specified; otherwise, it is ignored.
            The default character is the pipe symbol.  To suppress display of column borders, specify
            a blank space as the character to use.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="pageBreak" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The pageBreak attribute specifies whether to insert a page break between pivot tables. If
            all layers of a multilayer pivot table are exported, a page break will be inserted between
            the layers.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="imageFormat" type="oex:imageFormat" default="JPG">
        <xs:annotation>
          <xs:documentation>
            The imageFormat attribute specifies the image file format to use for exported graphics.
            A reference to each image file will be placed in the text document.
            The filename specified for the documentfile attribute is used as the rootname for the image
            files. A numeral is appended to the rootname to distinguish multiple image files from one
            another (e.g., MyOutput0.png, MyOutput1.png, etc).
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Information residing on the XLS subcommand -->
  <xs:element name="xls">
    <xs:annotation>
      <xs:documentation>
        The xls element is used to specify options for exporting to Excel 97-2003 documents (*.xls).
        Pivot table rows, columns, and cells are exported as Excel rows, columns, and cells, with all
        formatting attributes intact--for example, cell borders, font styles, and background colors.
        Text output is exported with all font attributes intact. Each line in the text output is a row
        in the Excel file, with the entire contents of the line contained in a single cell. Graphics are
        included in PNG format.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attribute name="documentfile" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The documentfile attribute specifies the name of the document file that will receive the
            exported contents.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="operation" default="createFile">
        <xs:annotation>
          <xs:documentation>
            The operation attribute specifies the type of operation to be performed by the export.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="createFile">
              <xs:annotation>
                <xs:documentation>
                                    Create a file. This is the default choice. Statistics will create an Excel file
                                    using the path and filename specified in the documentfile attribute and will
                                    export items to a single worksheet. If the specified file already exists,
                                    it will be overwritten.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="createSheet">
              <xs:annotation>
                <xs:documentation>
                                    Create a worksheet. Statistics will create a worksheet in the file specified in the
                                    documentfile attribute and will export items to that worksheet. If a worksheet
                                    having the name specified in the sheet attribute already exists, that
                                    worksheet will be overwritten.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="modifySheet">
              <xs:annotation>
                <xs:documentation>
                                    Modify a worksheet. Statistics will export items to the worksheet specified in the sheet attribute.
                                </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="sheet" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The sheet attribute specifies the name of the worksheet to which items will be exported.
            If operation=modifysheet is specified, the sheet attribute is required and omitting it
            causes an error. If operation=createfile or operation=createsheet is specified, the sheet
            keyword is optional. If it is omitted, the Excel default worksheet name will be used.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="location" default="lastColumn">
        <xs:annotation>
          <xs:documentation>
            The location attribute specifies the worksheet location at which the first exported item will start.
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="lastColumn">
              <xs:annotation>
                <xs:documentation>
                          Add exported items after the last column. This is the default choice. Exported
                          items will be added after the last column without modifying any existing
                          contents. This is a good choice for adding new columns to an existing worksheet.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="lastRow">
              <xs:annotation>
                <xs:documentation>
                          Add exported items below the last row. Exported items will be added below the
                          last row without modifying any existing contents. This is a good choice for
                          adding new rows to an existing worksheet.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="startCell">
              <xs:annotation>
                <xs:documentation>
                          Add exported items at a specific cell location. Exported items will be added
                          starting at the cell specified in the startCell attribute. Existing contents
                          may be overwritten.
                        </xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="startCell" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            Specifies the starting cell (e.g. "A1").  Only used if location is set to "startCell"
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="notesCaptions" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The notesCaptions keyword specifies whether to export footnotes and captions for pivot tables.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
