        <Parameter Name="SPANNED" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="LRECL" EqualsSign="Optional"/>
    <Subcommand Name="ENCODING" EqualsSign="Optional"/>
</Command>