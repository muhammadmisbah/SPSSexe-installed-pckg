<Command xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xml.spss.com/spss/extension http://xml.spss.com/spss/extension/SyntaxSchema.xsd"
    xmlns="http://xml.spss.com/spss/extension" Name="CASEPLOT" Language="Syntax" IsArbitrary="No">
    <Subcommand Name="GLOBAL_PARAMS">
        <Parameter Name="VARIABLES" EqualsSign="Optional" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="DIFF" EqualsSign="Optional"/>
    <Subcommand Name="SDIFF" EqualsSign="Optional"/>
    <Subcommand Name="PERIOD" EqualsSign="Optional"/>
    <Subcommand Name="NOLOG" EqualsSign="None"/>
    <Subcommand Name="LN" EqualsSign="None"/>
    <Subcommand Name="ID" EqualsSign="Optional"/>
