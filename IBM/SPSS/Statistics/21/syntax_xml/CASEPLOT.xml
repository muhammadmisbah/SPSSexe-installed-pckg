        <Parameter Name="SCALE" ParameterType="Keyword" EqualsSign="None"/>
    </Subcommand>
    <Subcommand Name="APPLY" EqualsSign="Optional"/>
    <Subcommand Name="FORMAT" EqualsSign="Optional">
        <Parameter Name="NOFILL" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="LEFT" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="NOREFERENCE" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="REFERENCE" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="NOJOIN" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="JOIN" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="HILO" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
</Command>
