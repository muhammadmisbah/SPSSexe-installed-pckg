 <!-- Created 5/21/08 -->
<Command xmlns="http://xml.spss.com/spss/extension"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xml.spss.com/spss/extension http://xml.spss.com/spss/extension/SyntaxSchema.xsd"
    Name="SAVE" Language="Syntax" IsArbitrary="No" EndRequired="None">
    <Subcommand Name="GLOBAL_PARAMS" EqualsSign="None">
        <Parameter Name="OUTFILE" EqualsSign="Optional"/>
    </Subcommand>
    <Subcommand Name="VERSION" EqualsSign="Optional"/>
    <Subcommand Name="UNSELECTED" EqualsSign="Optional">
        <Parameter Name="RETAIN" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="DELETE" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="KEEP" EqualsSign="Optional">
        <Parameter Name="ALL" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="DROP" EqualsSign="Optional"/>
    <Subcommand Name="RENAME" EqualsSign="Optional"/>
    <Subcommand Name="MAP" EqualsSign="None"/>
    <Subcommand Name="COMPRESSED" EqualsSign="None"/>
    <Subcommand Name="UNCOMPRESSED" EqualsSign="None"/>
	<Subcommand Name="ZCOMPRESSED" EqualsSign="None"/>
    <Subcommand Name="NAMES" EqualsSign="None"/>
    <Subcommand Name="PERMISSIONS" EqualsSign="Optional">
        <Parameter Name="READONLY" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="WRITEABLE" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="PASSPROTECT" EqualsSign="None">
        <Parameter Name="PASSWORD" EqualsSign="Required" ParameterType="Keyword">
            <EnumValue Name="NONE"/>
        </Parameter>
        <Parameter Name="ENCRYPTEDPW" EqualsSign="Required" ParameterType="Keyword">
            <EnumValue Name="YES"/>
            <EnumValue Name="NO"/>
        </Parameter>
    </Subcommand>
</Command>
