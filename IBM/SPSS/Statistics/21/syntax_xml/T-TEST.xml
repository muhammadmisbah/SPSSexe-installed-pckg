<Command xmlns="http://xml.spss.com/spss/extension"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xml.spss.com/spss/extension http://xml.spss.com/spss/extension/SyntaxSchema.xsd"
    Name="T-TEST" Language="Syntax" IsArbitrary="No" EndRequired="None">
    <Subcommand Name="GLOBAL_PARAMS" EqualsSign="None">
        <Parameter Name="TESTVAL" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="GROUPS" EqualsSign="Optional" ParameterType="Keyword"/>
        <Parameter Name="PAIRS" EqualsSign="Optional">
            <EnumValue Name="WITH"/>
            <EnumValue Name="PAIRED"/>
        </Parameter>
    </Subcommand>
    <Subcommand Name="TESTVAL" EqualsSign="Optional"/>
    <Subcommand Name="VARIABLE" EqualsSign="Optional"/>
    <Subcommand Name="VARIABLES" EqualsSign="Optional"/>
    <Subcommand Name="MISSING" EqualsSign="Optional">
        <Parameter Name="ANALYSIS" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="LISTWISE" EqualsSign="None" ParameterType="Keyword"/>
        <Parameter Name="INCLUDE" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
    <Subcommand Name="CRITERIA" EqualsSign="Optional">
        <Parameter Name="CI" EqualsSign="None" ParameterType="Keyword"/>
    </Subcommand>
</Command>
